#!/bin/bash

SPEC_PATH=../../awslabs/aws-sdk-go/apis
TEMPLATE_PATH=priv/json-service.ex.eex
OUTPUT_PATH=../aws-elixir/lib/aws

mix compile
mix run generate.exs CloudHSM \
    $SPEC_PATH/cloudhsm/2014-05-30/api-2.json \
    $SPEC_PATH/cloudhsm/2014-05-30/docs-2.json \
    $TEMPLATE_PATH > $OUTPUT_PATH/cloud_hsm.ex
