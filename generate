    $SPEC_PATH/cognito-identity/2014-06-30/api-2.json \
    $SPEC_PATH/cognito-identity/2014-06-30/docs-2.json \
    $TEMPLATE_PATH > $OUTPUT_PATH/cognito_identity.ex
