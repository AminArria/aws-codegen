    $SPEC_PATH/cloudtrail/2013-11-01/api-2.json \
    $SPEC_PATH/cloudtrail/2013-11-01/docs-2.json \
    $TEMPLATE_PATH > $OUTPUT_PATH/cloud_trail.ex
